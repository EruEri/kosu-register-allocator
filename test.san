def factoriel($n: ssize) ssize:
    $cmp = $n <= 1;
    if $cmp goto ending
    goto continue
ending:
    return 1
continue:
    $sub = $n - 1;
    $res = factoriel( $sub );
    $res2 = $n * $res;
    return $res2
end

